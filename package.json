    "express": "^4.13.4",
    "express-router": "0.0.1",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "jquery": "^3.0.0",
    "materialize-css": "^0.97.6",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "url": "^0.11.0",
    "velocity-animate": "^1.2.3",
    "whatwg-fetch": "^1.0.0"
  },
