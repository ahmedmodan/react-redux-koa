{
  "name": "react-redux-koa",
  "version": "1.0.0",
  "description": "a production ready React, Redux and Koa Boilerplate",
  "main": "server/server.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "6.2.1"
  },
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/karma start karma.config.js",
    "test:dev": "npm run test -- --watch",
    "clean": "rm -rf dist",
    "webpack-prod": "NODE_ENV=production webpack --progress -p",
    "server-prod": "NODE_ENV=production node server/server.js",
    "dev": "npm run clean && NODE_ENV=development webpack --progress && nodemon server/server.js",
    "start": "npm run clean && npm run webpack-prod && npm run test && npm run server-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedmodan/react-redux-koa.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "boilerplate"
  ],
  "author": "Ahmed Modan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahmedmodan/react-redux-koa/issues"
  },
  "homepage": "https://github.com/ahmedmodan/react-redux-koa#readme",
